// interval:seconds:10
i:s:10 {
    unroll(3) {
        print("Triple double!")
    }
}

// Wildcard usage
kprobe:tcp_reset, kprobe:acpi_thermal* {
    printf("Entered: %s\n", probe);
}

kprobe:arp_create {
  $sip = arg4;
  $dip = arg2;
  $smac = sarg0;
  $dmac = sarg1;
  time("%H:%M:%S ");
  printf("SRC: %16s %s", ntop($sip), macaddr($smac));
  printf(" -> DST: %16s %s\n", ntop($dip), macaddr($dmac));
  printf("\n%s- %s- %s\n", macaddr(sarg0), macaddr(sarg1), ntop(sarg2));
}