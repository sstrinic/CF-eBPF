// interval:seconds:1
i:s:1 {
  unroll(3) {
    print("Unrolled")
  }
}

// Wildcard usage
kprobe:tcp_reset,kprobe:*socket* {
  printf("Entered: %s\n", probe);
}

kprobe:arp_create {
  $stack_arg0 = *(uint8*)(reg("sp") + 8);
  $stack_arg1 = *(uint8*)(reg("sp") + 16);
  printf("SRC %s, DST %s\n", macaddr($stack_arg0), macaddr($stack_arg1));
}