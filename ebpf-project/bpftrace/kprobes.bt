// interval:seconds:10
i:s:20 {
    unroll(3) {
        print("Triple double!")
    }
}

// Wildcard usage
kprobe:tcp_reset, kprobe:acpi_thermal* {
    printf("Entered: %s\n", probe);  // Print used probe
}

kprobe:arp_create {
  $sip = arg4;
  $dip = arg2;
  $smac = sarg0; // arg6
  $dmac = sarg1; // arg5
  time("%H:%M:%S ");
  printf("SRC: %16s %s", ntop($sip), macaddr($smac));
  printf(" -> DST: %16s %s\n", ntop($dip), macaddr($dmac));
}
